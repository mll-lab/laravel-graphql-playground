{
  "name": "mll-lab/laravel-graphql-playground",
  "description": "Easily integrate GraphQL Playground into your Laravel project",
  "license": "MIT",
  "keywords": [
    "laravel",
    "graphql",
    "graphql-playground"
  ],
  "authors": [
    {
      "name": "Benedikt Franke",
      "email": "benedikt@franke.tech"
    }
  ],
  "require": {
    "php": "^7.1 || ^8",
    "illuminate/console": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6 || ^7 || ^8 || ^9",
    "illuminate/contracts": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6 || ^7 || ^8 || ^9",
    "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6 || ^7 || ^8 || ^9"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2",
    "laravel/lumen-framework": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6 || ^7 || ^8 || ^9",
    "mll-lab/php-cs-fixer-config": "^4.4"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "MLL\\GraphQLPlayground\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "ergebnis/composer-normalize": true
    },
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "MLL\\GraphQLPlayground\\GraphQLPlaygroundServiceProvider"
      ]
    }
  }
}
